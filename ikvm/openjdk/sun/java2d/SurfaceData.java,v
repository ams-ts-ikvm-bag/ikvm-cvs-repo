package sun.java2d;

public class SurfaceData {

	final Object destination;
	
	public SurfaceData( Object destination ) {
		this.destination = destination;
	}
	
	public boolean isSurfaceLost() {
		return false;
	}

	public Object getDestination() {
		return destination;
	}
}
@


